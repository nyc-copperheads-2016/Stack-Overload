<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <p></p>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit 'Join' %>

<% end %>
<% if defined?(@user.errors) %>
  <% @user.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
<% end %>
